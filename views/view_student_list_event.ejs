<body style="background-color: rgb(237, 241, 255);">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="/pc_events">
            <i class="bi bi-arrow-left-circle"></i> Back
        </a>
    </nav>

    <h3 style="padding:20px;padding-left:550px">Students List</h3>
    <hr>
    <!-- Table -->
    <div class="container">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">USN</th>
                    <th scope="col">Name</th>
                    <th scope="col">Branch</th>
                    <th scope="col">Email</th>
                    <th scope="col">Event Name</th>
                    <th scope="col">Company Name</th>
                    <th scope="col">Date of Event</th>
                </tr>
            </thead>
            <tbody>
                <!-- Loop through students and display their information -->
                <% for(let student of result) { %>
                    <% const eventDate=new Date(student.studate_of_event)%>
                        <%if(student.studate_of_event>=Date.now()){%>
                            <tr>
                                <td>
                                    <%= student.stuusn %>
                                </td>
                                <td>
                                    <%= student.stufirstname %>
                                        <%= student.stulastname %>
                                </td>
                                <td>
                                    <%= student.stubranch %>
                                </td>
                                <td>
                                    <%= student.stuemail %>
                                </td>
                                <td>
                                    <%= student.stucompany_name %>
                                </td>
                                <td>
                                    <%= student.stuevent_name %>
                                </td>
                                <td>
                                    <%= student.studate_of_event.toDateString() %>
                                </td>
                            </tr>
                            <% } %>
                                <% } %>
            </tbody>
        </table>


    </div>






</body>

</html>